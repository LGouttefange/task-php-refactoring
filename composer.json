{
    "name": "lgouttefange/task-php-refactoring",
    "type": "project",
    "require": {
        "symfony/console": "^7.1",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/cache": "^7.1",
        "symfony/dotenv": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
